{
  "name": "iprotrax",
  "version": "8.1.0",
  "description": "AI-Powered Production Tracker",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "watcher": "npx tsx scripts/watcher.ts",
    "dev:watch": "concurrently \"npm run dev\" \"npm run watcher\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "docker:up": "docker-compose up -d --build",
    "docker:build:amd64": "docker build --platform linux/amd64 -t hzhang9/iprotrax:latest .",
    "docker:push": "docker push hzhang9/iprotrax:latest",
    "test": "vitest",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "6.19.1",
    "bcryptjs": "^3.0.3",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "html5-qrcode": "^2.3.8",
    "ioredis": "^5.9.2",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-intl": "^4.7.0",
    "openai": "^6.16.0",
    "prisma": "6.19.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.6.0",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "husky": "^9.1.7",
    "javascript-obfuscator": "^5.1.0",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-obfuscator": "^3.6.0"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "overrides": {
    "diff": "8.0.3"
  }
}